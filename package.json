{
	"name": "node-backend",
	"version": "1.0.0",
	"description": "Node backend",
	"main": "server.js",
	"scripts": {
		"start": "babel-node ./server.js --presets es2015,stage-2",
		"migrate-up": "babel-node --presets es2015,stage-2 ./node_modules/.bin/sequelize db:migrate",
		"migrate-down": "babel-node --presets es2015,stage-2 ./node_modules/.bin/sequelize db:migrate:undo",
		"migrate-down-all": "babel-node --presets es2015,stage-2 ./node_modules/.bin/sequelize db:migrate:undo:all",
		"dev": "nodemon server.js --exec babel-node --presets es2015,stage-2",
		"server": "babel-node ./server.js --presets es2015,stage-2",
		"dev-monster": "npm run migrate-up && npm run dev",
		"server-monster": "npm run migrate-up && npm run server",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"engines": {
		"node": "^8.9.1",
		"npm": "^5.6.0"
	},
	"author": "ayush_shukla@live.com",
	"license": "MIT",
	"dependencies": {
		"airbrake-js": "^1.6.2",
		"async": "^3.2.2",
		"aws-sdk": "^2.264.1",
		"axios": "^0.16.2",
		"basic-auth": "^2.0.0",
		"better-queue": "^3.8.10",
		"bignumber.js": "^8.0.1",
		"bitcore-lib": "^8.3.4",
		"body-parser": "^1.15.2",
		"cookie": "^0.3.1",
		"cookie-parser": "^1.4.3",
		"cors": "^2.8.3",
		"cross-fetch": "^3.0.0",
		"crypto-js": "^3.1.9-1",
		"csv": "^5.1.1",
		"eth-balance-checker": "^0.1.1",
		"ethereumjs-tx": "1.3.7",
		"express": "^4.14.0",
		"express-fileupload": "^0.4.0",
		"joi": "^13.4.0",
		"jsonwebtoken": "^8.3.0",
		"kafka-node": "^3.0.1",
		"key-del": "^1.3.0",
		"keythereum": "^1.0.4",
		"kue": "^0.11.6",
		"kue-ui-express": "^1.0.2",
		"md5": "^2.2.1",
		"moment": "^2.21.0",
		"mongoose": "^5.5.11",
		"node-cron": "^1.2.1",
		"pg": "^6.4.2",
		"pg-hstore": "^2.3.2",
		"pusher": "^2.1.3",
		"randomstring": "^1.1.5",
		"redis": "^2.8.0",
		"redlock": "^3.1.2",
		"sequelize": "^4.28.6",
		"sequelize-cli": "^5.5.0",
		"sequelize-replace-enum-postgres": "^1.4.0",
		"speakeasy": "^2.0.0",
		"uuid": "^3.2.1",
		"web3": "^1.0.0-beta.55"
	},
	"devDependencies": {
		"babel-cli": "^6.11.4",
		"babel-preset-es2015": "^6.9.0",
		"babel-preset-stage-2": "^6.11.0",
		"babel-register": "^6.11.6",
		"nodemon": "^1.11.0",
		"pm2": "^2.7.1"
	}
}
